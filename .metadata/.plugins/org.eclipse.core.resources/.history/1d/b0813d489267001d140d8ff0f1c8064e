//----------------- CONSULTAR SOBRE PESO DE ARCHIVOS CSV ------------------------------

#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main(void) {

	setbuf(stdout,NULL);
	srand(50);
	int num= rand(time(NULL));
	printf("%d", num);

	for(int i=0; i<10;i++)
	{
		printf("%d\s", num);
	}

	return EXIT_SUCCESS;
}
